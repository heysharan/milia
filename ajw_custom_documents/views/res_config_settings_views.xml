<odoo>
    <record id="res_config_settings_view_form_inherit" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="documents.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//block[@id='files_centralization_section']" position="after">
                <group>
                    <block title="MSME Certificate">
                        <setting id="notify_before" string="Notify before (days)"
                                 help="Set msme certificate notify before expire">
                            <div class="content-group">
                                <div class="row mt16">
                                    <field class="w-50" name="msme_cert_expiry_notify_before"/>
                                </div>
                            </div>
                        </setting>
                    </block>
                </group>

            </xpath>
        </field>
    </record>
</odoo>